
@{
    Layout = null;
}

@model WebApplication7.Models.Account

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h3>Register</h3>
    <form asp-controller="account" method="post" enctype="multipart/form-data">
        <table>
            <tr>
                <td>Username</td>
                <td>
                    <input type="text" asp-for="Username" />
                </td>
                <td>
                    <span asp-validation-for="Username"></span>
                </td>
            </tr>

            <tr>
                <td>Password</td>
                <td>
                    <input type="text" asp-for="Password" />
                </td>
                <td>
                    <span asp-validation-for="Password"></span>
                </td>
            </tr>

            <tr>
                <td>Email</td>
                <td>
                    <input type="text" asp-for="Email" />
                </td>
                <td>
                    <span asp-validation-for="Email"></span>
                </td>
            </tr>

            <tr>
                <td>Website</td>
                <td>
                    <input type="text" asp-for="Website" />
                </td>
                <td>
                    <span asp-validation-for="Website"></span>
                </td>
            </tr>

            <tr>
                <td>Age</td>
                <td>
                    <input type="text" asp-for="Age" />
                </td>
                <td>
                    <span asp-validation-for="Age"></span>
                </td>
            </tr>

            <tr>
                <td>&nbsp;</td>
                <td>
                    <input type="submit" value="save" asp-action="save" />
                </td>
            </tr>
        </table>
    </form>
</body>
</html>
